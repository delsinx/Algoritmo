algoritmo "Exercicio 11"
var
     a, b, c, calculo : real
inicio
        escreval("insira o valor de a : ")
        leia(a)
        escreval("insira o valor de b : ")
        leia(b)
        escreval("insira o valor de c : ")
        leia(c)
        calculo <- b^2 - 4 * a * c
        escreval (calculo)
fimalgoritmo