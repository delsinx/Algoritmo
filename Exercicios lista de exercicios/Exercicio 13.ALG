algoritmo "Exercicio 13"
var   salario, calculo1, calculo2 : real

inicio
           escreval("insira o salario do funcionario : ")
           leia(salario)
           calculo1 <- salario*15/100
           calculo2 <- salario+calculo1
           escreval("O salario com o aumento de 15% ficas : ",calculo2)
fimalgoritmo