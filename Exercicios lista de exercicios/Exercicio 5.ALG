algoritmo "Exercicio5"
var   nota1, nota2, media : real

inicio
         escreval("insira a primeira nota : ")
         leia(nota1)
         escreval("insira a segunda nota : ")
         leia(nota2)
         media <- (nota1+nota2)/2
         escreval ("A media entre ",nota1," e ",nota2," é : ",media)
fimalgoritmo